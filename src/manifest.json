{
  "manifest_version": 2,
  "version": "1.0.1",
  "name": "LinkedIn Finder",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.meetup.com/*", "*://*.facebook.com/*"]
    }
  ],
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png" 
  }
}
